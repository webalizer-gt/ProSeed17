		<input name="RM_UPGRADE_RMleft"				category="VEHICLE" key1="KEY_z" 				key2="" button="" device="0" mouse="" visible="true" />	
		<input name="DRIVINGLINE_TOGGLESHUTOFF"		category="VEHICLE" key1="KEY_KP_9" 				key2="" button="" device="0" mouse="" visible="true" />	
