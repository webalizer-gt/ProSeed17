<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<modDesc descVersion="32">
   <author>gotchTOM/webalizer</author>
    <title>
		<en>ProSeed</en>
        <de>ProSeed</de>
		<fr>ProSeed</fr>
    </title>
    <description>
        <en>Additional functionality for sowing machines.</en>
		<de>Erweiterte Funktionalität für Sämaschinen.</de>
		<fr>Fonctionnalité supplémentaire pour les machines à coudre.</fr>
 	</description>
    <multiplayer supported="true"/>
	<iconFilename>store.png</iconFilename>
	<version>0.10.10</version>
	<extraSourceFiles>
        <sourceFile filename="register.lua" />
    </extraSourceFiles>
	<inputBindings>
		<input name="SOWINGSUPP_HUD"       			category="VEHICLE" key1="KEY_KP_7"   			key2="" button="" device="0" mouse="" visible="true" />
		<input name="SOWINGSUPP_MOUSE"       		category="VEHICLE" key1="KEY_lctrl"   			key2="" button="" device="0" mouse="" visible="true" />
		<input name="DRIVINGLINE"  					category="VEHICLE" key1="KEY_KP_8" 				key2="" button="" device="0" mouse="" visible="true" />	
